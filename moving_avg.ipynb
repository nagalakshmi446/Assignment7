    "ls = [3, 5, 7, 2, 8, 10, 11, 65, 72, 81, 99, 100, 150]\n",
    "window_size = 3\n",
    "i = 0\n",
    "moving_avg = []\n",
    "while i <len(ls) - window_size+1:\n",
    "    block = ls[i:i+window_size]\n",
    "    window_average = sum(block)/window_size\n",
    "    moving_avg.append(window_average)\n",
    "    i = i+1\n",
    "print(moving_avg)\n",
